{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "browserslist": [
    "Android >= 4",
    "Chrome >= 29",
    "iOS >= 6",
    "Safari >= 7.1"
  ],
  "scripts": {
    "build": "babel-node tools/cli build",
    "build:uat": "babel-node tools/cli build --env=uat",
    "build:prod": "babel-node tools/cli build --env=prod",
    "clean": "babel-node tools/cli clean",
    "start": "babel-node tools/cli start --dev",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "eslint \"{src,tools}/**/*.{js,jsx}\"",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sass}\""
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@byhealth/native-app-jssdk": "^2.5.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "extend": "^3.0.2",
    "normalize.css": "^8.0.0",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "preact-portal": "^1.1.3",
    "query-string": "^5.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "husky": "^1.0.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx}": [
        "eslint --no-ignore --fix",
        "git add --force"
      ],
      "*.{json,md}": [
        "prettier --write",
        "git add --force"
      ],
      "*.{css,less,scss,sass}": [
        "stylelint --fix",
        "git add --force"
      ]
    }
  },
  "author": "Wind4 <puxiaping@gmail.com>",
  "license": "MIT"
}
